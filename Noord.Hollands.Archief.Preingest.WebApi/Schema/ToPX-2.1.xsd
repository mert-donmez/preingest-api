<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.nationaalarchief.nl/ToPX/v2.1"
	xmlns="http://www.nationaalarchief.nl/ToPX/v2.1" elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="v2.1">
	<xs:element name="ToPX" type="topxType">
	</xs:element>
	<xs:complexType name="topxType">
		<xs:choice>
			<xs:element name="aggregatie" type="aggregatieType">
				<xs:annotation>
					<xs:documentation>Gegevens over een archief, serie, dossier of record.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bestand" type="bestandType">
				<xs:annotation>
					<xs:documentation>Gegevens over een bestand, waarbij vorm- en formaat-informatie verplicht meegeleverd dienen te
						worden.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:group name="basisEigenschappenGroup">
		<xs:sequence>
			<xs:element name="naam" type="typedNonEmptyStringType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Beknopte formeel-inhoudelijk beschrijving (titel) van het record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classificatie" type="classificatieType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Samenvoeging van archiefbestanddelen tot één nieuw geheel met een eigen identiteit.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="omschrijving" type="typedStringType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Vrije tekst met nadere beschrijving van record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plaats" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Fysieke of virtuele locatie van record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dekking" type="dekkingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Nadere aanduiding van tijd en ruimte waarin record kan worden gepositioneerd</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externIdentificatiekenmerk" type="externIdentificatiekenmerkType" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Kenmerken, toegekend aan een record, buiten de huidige beheeromgeving</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taal" type="taalType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Taal van een record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eventGeschiedenis" type="eventGeschiedenisType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Vastlegging van het ontstaan en wijzigingen van een record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eventPlan" type="eventPlanType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Geeft activiteiten of gebeurtenissen weer die in de toekomst van belang zijn voor het record
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relatie" type="relatieType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Element waarmee een relatie gelegd kan worden met andere records (op verschillende
						aggregatieniveaus)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="context" type="contextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gebruiksrechten" type="gebruiksrechtenType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Voorwaarden verbonden aan het gebruik van het record anders dan raadpleging</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vertrouwelijkheid" type="vertrouwelijkheidType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Indicatie van niveau van vertrouwelijkheid van de informatie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="openbaarheid" type="openbaarheidType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Mogelijke beperkingen aan de raadpleging</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="aggregatieType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Uniek kenmerk van een record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau" type="aggregatieAggregatieniveauType">
				<xs:annotation>
					<xs:documentation>Het niveau waarop een record kan worden beschreven</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="basisEigenschappenGroup" />
			<xs:element name="integriteit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Volledigheid van een record - juist, volledig, tijdig, geautoriseerd. N.B. Hier wordt de
						inhoudelijke integriteit bedoeld.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="generiekeMetadata" type="generiekeMetadataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Algemene metadata waarin ToPX 2.0 niet in voorziet. Dit element dus alleen gebruiken indien
						voorgaande elementen niet voldoen.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="bestandType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Uniek kenmerk van een record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau" type="bestandAggregatieniveauType">
				<xs:annotation>
					<xs:documentation>Het niveau waarop een record kan worden beschreven. Is in het geval van een bestand altijd
						'Stuk'.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="basisEigenschappenGroup" />
			<xs:element name="vorm" type="vormType">
				<xs:annotation>
					<xs:documentation>Redactie, verschijningsvorm en structuur van het record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integriteit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Volledigheid van een record - juist, volledig, tijdig, geautoriseerd. N.B. Hier wordt de
						inhoudelijke integriteit bedoeld.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="formaat" type="formaatType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Fysieke/technische aspecten van een record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="generiekeMetadata" type="generiekeMetadataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Algemene metadata waarin ToPX 2.0 niet in voorziet. Dit element dus alleen gebruiken indien
						voorgaande elementen niet voldoen.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="nonEmptyStringType">
		<xs:annotation>
			<xs:documentation>Een tekst waarde, die ingevuld dient te zijn (minimale lengte is 1).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typedNonEmptyStringType">
		<xs:simpleContent>
			<xs:extension base="nonEmptyStringType">
				<xs:attribute name="type" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="typedStringType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="type" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="aggregatieAggregatieniveauType">
		<xs:annotation>
			<xs:documentation>Enumeratie van 4 strings die de verschillende aggregatieniveau's beschrijven: archief, serie,
				dossier, record.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Record" />
			<xs:enumeration value="Dossier" />
			<xs:enumeration value="Serie" />
			<xs:enumeration value="Archief" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bestandAggregatieniveauType">
		<xs:annotation>
			<xs:documentation>Enumeratie met 1 waarde die geldig is voor een bestand: Stuk</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Stuk" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="classificatieType">
		<xs:sequence>
			<xs:element name="code" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Kenmerk waaronder het record wordt ingedeeld of geklasseerd</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="omschrijving" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Nadere omschrijving van classificatie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bron" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Verwijzing naar het geldende classificatieschema of ordeningsplan</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--Wout mailen over wijziging van datum naar datumOfPeriode -->
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Datum of periode wanneer geldig of gebruikt</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="dekkingType">
		<xs:sequence>
			<xs:element name="inTijd" type="periodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Geeft positionering in de tijd aan, waarin iets van kracht is of was dan wel bestond
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geografischGebied" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Geeft ruimtelijke afbakening of locatie van record of werking ervan aan</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="periodeType">
		<xs:sequence>
			<xs:element name="begin" type="datumOfJaarType" minOccurs="0" />
			<xs:element name="eind" type="datumOfJaarType" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="datumOfJaarType">
		<xs:choice>
			<xs:element name="jaar" type="xs:gYear" />
			<xs:element name="datum" type="xs:date" />
			<xs:element name="datumEnTijd" type="xs:dateTime" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="externIdentificatiekenmerkType">
		<xs:sequence>
			<xs:element name="kenmerkSysteem" type="xs:string" minOccurs="0" />
			<xs:element name="nummerBinnenSysteem" type="nonEmptyStringType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="eventGeschiedenisType">
		<xs:sequence>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Datum of periode waarop of -in iets heeft plaatsgevonden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Soort activiteit of gebeurtenis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="beschrijving" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nadere omschrijving van de gebeurtenis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verantwoordelijkeFunctionaris" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Degene die verantwoordelijk is voor de gebeurtenis of activiteit</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="datumOfPeriodeType">
		<xs:choice>
			<xs:element name="datum" type="xs:date" />
			<xs:element name="jaar" type="xs:gYear" />
			<xs:element name="datumEnTijd" type="xs:dateTime" />
			<xs:element name="periode" type="periodeType" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="eventPlanType">
		<xs:sequence>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Datum of periode waarop of -in iets zal plaatsvinden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="nonEmptyStringType" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Soort activiteit of gebeurtenis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="beschrijving" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nadere omschrijving van de gebeurtenis of activiteit</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aanleiding" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicatie van mechanisme waarop gebeurtenis of actie in gang wordt gezet</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="relatieType">
		<xs:sequence>
			<xs:element name="relatieID" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Identificatie van het record waarnaar de relatie gelegd wordt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeRelatie" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Nadere aanduiding van aard van de relatie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Datum of periode waarin de relatie bestaat</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contextType">
		<xs:sequence>
			<xs:element name="actor" type="actorType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Een organisatie of persoon verantwoordelijk voor of betrokken bij het opmaken, opnemen van
						archiefbescheiden en/of processen van informatie- en archiefbeheer
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="activiteit" type="activiteitType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Het geheel van taken, functies, activiteiten en transacties die op basis van een mandaat worden
						uitgevoerd door een actor
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="actorType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Uniek kenmerk van een actor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Onderscheidt de niveaus waarop een actor kan worden beschreven</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geautoriseerdeNaam" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>De benaming(en), waaronder een actor bekend is</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plaats" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jurisdictie" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nadere omschrijving van werkingsgebied of bevoegdheden</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="activiteitType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>Uniek kenmerk van een activiteit</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Onderscheidt de niveaus waarop een activiteit kan worden beschreven</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naam" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Kernachtige omschrijving van de activiteit of het proces</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="gebruiksrechtenType">
		<xs:sequence>
			<xs:element name="omschrijvingVoorwaarden" type="nonEmptyStringType" default="geen gebruiksrechten">
				<xs:annotation>
					<xs:documentation>Voorwaarden voor het gebruik anders dan raadpleging van het record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Datum of periode waarin voorwaarden van toepassing zijn</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="vertrouwelijkheidType">
		<xs:sequence>
			<xs:element name="classificatieNiveau" type="classificatieNiveauType">
				<xs:annotation>
					<xs:documentation>Aanduiding van niveau van vertrouwelijkheid</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Datum of periode waarin het niveau van vertrouwelijkheid van toepassing is</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="classificatieNiveauType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Niet vertrouwelijk" />
			<xs:enumeration value="Departementaal vertrouwelijk" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="openbaarheidType">
		<xs:sequence>
			<xs:element name="omschrijvingBeperkingen" type="nonEmptyStringType" default="openbaar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Omschrijving van beperkingen voor raadpleging</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Datum of periode waarin openbaarheidsbeperking van toepassing is</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="vormType">
		<xs:sequence>
			<xs:element name="redactieGenre" type="redactieGenreType">
				<xs:annotation>
					<xs:documentation>Type archiefbescheiden (records) met een specifieke stijl, inhoud of vorm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verschijningsvorm" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Verschijningsvorm van een Record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="structuur" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Omschrijving van structuur van record (op verschillende aggregatieniveaus)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="formaatType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringType">
				<xs:annotation>
					<xs:documentation>Uniek kenmerk van digitaal bestand</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bestandsnaam" type="bestandsnaamType">
				<xs:annotation>
					<xs:documentation>Korte omschrijving van een digitaal bestand of benaming</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Wijze van groepering van bestanden omwille van samenhang of verpakking. Typering van digitaal
						bestand</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="omvang" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ruimtebeslag op medium, meestal uitgedrukt in bytes of een veelvoud ervan</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bestandsformaat" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code volgens welke gegevens op een gegevensdrager zijn opgeslagen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creatieapplicatie" type="creatieApplicatieType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Omschrijving van de applicatie waarmee het bestand oorspronkelijk gemaakt is</xs:documentation>
				</xs:annotation>
			</xs:element>
